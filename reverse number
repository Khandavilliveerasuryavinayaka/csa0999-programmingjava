import java.io.*;
import java.util.*;
class reverse
{
public static void main(String args[])
{
int n,reverse=0,remainder;
Scanner s=new Scanner(System.in);
System.out.print("enter the n value");
n=s.nextInt();
while(n!=0)
{
remainder=n%10;
reverse = reverse*10+remainder;
n=n/10;
}
System.out.print("reverse number"+reverse);
}
}
