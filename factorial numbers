import java.io.*;
import java.util.*;
class fact
{
 public static void main(String args[])
{
 int fact=1,i=1,n;
 Scanner z = new Scanner(System.in);
 System.out.println("enter the factorial number");
 n=z.nextInt();
 for(i=1;i<n;i++)
 fact=fact*i;
System.out.println(" the factorial number"+fact);
}
}
